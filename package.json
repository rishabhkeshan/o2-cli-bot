{
  "name": "o2-cli-bot",
  "version": "1.0.4",
  "description": "CLI-based automated trading bot for O2 Exchange on Fuel Network",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "o2-bot": "dist/index.js"
  },
  "files": [
    "dist/",
    "strategies/"
  ],
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && cp src/types/contracts/orderbook-abi.json dist/types/contracts/",
    "prepublishOnly": "npm run build",
    "start": "node dist/index.js",
    "start:no-tui": "node dist/index.js --no-tui",
    "bot": "node dist/index.js",
    "markets": "node dist/index.js markets",
    "history": "node dist/index.js history"
  },
  "keywords": [
    "o2",
    "fuel",
    "trading-bot",
    "dex",
    "cli",
    "orderbook",
    "fuel-network"
  ],
  "author": "rishabhkeshan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rishabhkeshan/o2-cli-bot.git"
  },
  "homepage": "https://github.com/rishabhkeshan/o2-cli-bot#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axios": "^1.7.0",
    "blessed": "^0.1.81",
    "commander": "^12.1.0",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.4.7",
    "ethers": "^6.15.0",
    "fuels": "^0.102.0",
    "node-telegram-bot-api": "^0.66.0",
    "sql.js": "^1.11.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/blessed": "^0.1.25",
    "@types/node": "^20.14.0",
    "@types/node-telegram-bot-api": "^0.64.7",
    "@types/sql.js": "^1.4.9",
    "@types/ws": "^8.5.12",
    "tsx": "^4.19.0",
    "typescript": "^5.5.0"
  }
}
